#include<iostream>
using namespace std;


int main(){
int Rows, Columns;
cout<<"Enter no. of Rows: ";
cin>>Rows;
cout<<"Enter no. of Columns: ";
cin>>Columns;
for(int i=1; i<=Rows;i++){
    for(int j=1; j<=Columns;j++){
        if(i==1||i==Rows||j==1||j==Columns){
        cout<<"*";}
        else{cout<<" ";}
    }
    cout<<endl;
}

    
    
    return 0;

}
